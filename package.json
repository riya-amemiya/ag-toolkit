{
	"name": "ag-toolkit",
	"version": "0.4.1",
	"description": "Shared library for agbd and agrb",
	"license": "MIT",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/riya-amemiya/ag-toolkit.git"
	},
	"homepage": "https://github.com/riya-amemiya/ag-toolkit#readme",
	"bugs": {
		"url": "https://github.com/riya-amemiya/ag-toolkit/issues"
	},
	"exports": {
		".": "./dist/index.js"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "bun test",
		"test:watch": "bun test --watch",
		"lint": "biome check . --write",
		"ci": "biome ci . && bun test",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"ink": "6.6.0",
		"ink-select-input": "6.2.0",
		"ink-spinner": "5.0.0",
		"react": "19.2.3",
		"simple-git": "3.30.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.11",
		"@sindresorhus/tsconfig": "8.1.0",
		"@types/bun": "1.3.5",
		"@types/react": "19.2.8",
		"chalk": "5.6.2",
		"ink-testing-library": "4.0.0",
		"ts-node": "10.9.2",
		"typescript": "5.9.3"
	}
}
